{
  "js": {
    "name": "JavaScript",
    "color": "#f7e018",
    "libraries": {
      "util": {
        "docs": "https://spec-tacles.github.io/util.js/{version}",
        "versions": [
          "master"
        ],
        "description": "A collection of Discord and Spectacles utilities.",
        "installation": "npm i @spectacles/util"
      },
      "rest": {
        "docs": "https://spec-tacles.github.io/rest.js/{version}",
        "versions": [
          "master"
        ],
        "description": "A Discord REST client with support for distributed ratelimiting.",
        "installation": "npm i @spectacles/rest"
      }
    }
  },
  "go": {
    "name": "Go",
    "color": "#00acd7",
    "inverted": true,
    "libraries": {
      "brokers": {
        "repo": "https://github.com/spec-tacles/go",
        "docs": "https://godoc.org/github.com/spec-tacles/go/broker",
        "description": "Broker libraries for Go applications.",
        "installation": "go get github.com/spec-tacles/go/brokers"
      },
      "rest": {
        "repo": "https://github.com/spec-tacles/go",
        "docs": "https://godoc.org/github.com/spec-tacles/go/rest",
        "description": "A basic Discord REST library for Go applications.",
        "installation": "go get github.com/spec-tacles/go/rest"
      },
      "types": {
        "repo": "https://github.com/spec-tacles/go",
        "docs": "https://godoc.org/github.com/spec-tacles/go/types",
        "description": "Go types for Discord data structures.",
        "installation": "go get github.com/spec-tacles/go/types"
      }
    }
  },
  ".net": {
    "name": ".NET",
    "color": "#178600",
    "inverted": true,
    "libraries": {
      "gateway": {
        "repo": "https://github.com/spec-tacles/Spectacles.NET",
        "versions": [
          "master",
          "stable"
        ],
        "docs": "https://spec-tacles.github.io/Spectacles.NET/{versions}/api/Spectacles.NET.Gateway",
        "description": "Gateway library for .NET applications.",
        "installation": "Install-Package Spectacles.NET.Gateway"
      },
      "broker": {
        "repo": "https://github.com/spec-tacles/Spectacles.NET",
        "versions": [
          "master",
          "stable"
        ],
        "docs": "https://spec-tacles.github.io/Spectacles.NET/{version}/api/Spectacles.NET.Broker",
        "description": "Broker library for .NET applications.",
        "installation": "Install-Package Spectacles.NET.Broker"
      },
      "rest": {
        "repo": "https://github.com/spec-tacles/Spectacles.NET",
        "versions": [
          "master",
          "stable"
        ],
        "docs": "https://spec-tacles.github.io/Spectacles.NET/{version}/api/Spectacles.NET.Rest",
        "description": "A Discord REST client with support for distributed ratelimiting.",
        "installation": "Install-Package Spectacles.NET.Rest"
      },
      "types": {
        "repo": "https://github.com/spec-tacles/Spectacles.NET",
        "versions": [
          "master",
          "stable"
        ],
        "docs": "https://spec-tacles.github.io/Spectacles.NET/{version}/api/Spectacles.NET.Types",
        "description": "Discord API .NET Typings.",
        "installation": "Install-Package Spectacles.NET.Types"
      }
    }
  }
}
